{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "src",
    "FLASK_DEBUG": "false",
    "SECRET_KEY": "production-secret-key-hargi-dashboard-2024",
    "CORS_ORIGINS": "*",
    "EXCEL_FILE_PATH": "./DASHBOARDHARGI.xlsx"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  }
}